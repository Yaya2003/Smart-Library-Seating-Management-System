<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.ReservationMapper">

    <select id="countByCreatedAtBetween" resultType="long">
        SELECT COUNT(1)
        FROM reservation
        WHERE date = #{date}
          AND created_at &gt;= #{start}
          AND created_at &lt; #{end}
    </select>

</mapper>

